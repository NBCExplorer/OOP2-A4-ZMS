<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop2a4zms.Controllers.CompositeAnimalCollectionViewController">
   <children>
      <ListView fx:id="aAnimalCollectionListView" prefHeight="338.0" prefWidth="200.0" />
      <VBox alignment="CENTER" prefHeight="306.0" prefWidth="240.0" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
          <Button fx:id="aViewButton" onAction="#onViewButtonClick" text="View" />
         <Button fx:id="aBackButton" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back/Exit" />
      </VBox>
   </children>
</HBox>
